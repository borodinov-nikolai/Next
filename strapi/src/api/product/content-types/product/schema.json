{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "buyURL": {
      "type": "string",
      "required": true
    },
    "productID": {
      "type": "string"
    },
    "platform": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::platform.platform",
      "inversedBy": "products"
    },
    "genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "mappedBy": "products"
    },
    "price": {
      "type": "decimal",
      "required": true
    }
  }
}
